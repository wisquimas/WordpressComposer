{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "*",
    "fancyguy/wordpress-monolog": "dev-master",
    "wpackagist-plugin/magic-fields-2": "*"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp/wp-content/plugins/{$name}/": [
        "wpackagist-plugin/magic-fields-2",
        "fancyguy/wordpress-monolog"
      ]
    }
  }
}